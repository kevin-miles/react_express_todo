    "hbs": "~4.0.0",
    "mongodb": "^2.2.5",
    "morgan": "~1.7.0",
    "node-sass-middleware": "0.8.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "concurrently": "^2.2.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
