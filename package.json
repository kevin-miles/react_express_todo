{
  "name": "react_express_todo",
  "description": "A todo app written with express + react + mongo",
  "author": "kevin-miles",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "webpack && node ./bin/www",
    "build": "webpack --progress",
    "webpack-watch": "webpack -w",
    "express-server": "node ./server",
